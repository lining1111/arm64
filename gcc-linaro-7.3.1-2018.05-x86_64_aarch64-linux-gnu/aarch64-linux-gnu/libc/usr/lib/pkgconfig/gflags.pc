prefix=/usr/local/arm64/gcc-linaro-7.3.1-2018.05-x86_64_aarch64-linux-gnu/aarch64-linux-gnu/libc/usr
exec_prefix=${prefix}
bindir=${exec_prefix}/bin
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: gflags
Version: 2.2.2
Description: A commandline flags library that allows for distributed flags.
URL: http://gflags.github.io/gflags
Requires:
Libs: -L${libdir} -lgflags
Libs.private: -lpthread 
Cflags: -I${includedir}
